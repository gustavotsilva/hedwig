    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bootstrap": "^5.0.5",
    "@types/mongodb": "^3.6.8",
    "@types/react-scroll": "^1.8.2"
